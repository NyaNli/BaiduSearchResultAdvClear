{
    "name": "百度搜索广告清理",
    "version": "3.1415926",
    "manifest_version": 2,
    "content_scripts":[
        {
            "matches":[
                "https://www.baidu.com/s?*",
                "http://www.baidu.com/s?*"
            ],
            "js":["ClearAdv.js"],
            "run_at": "document_start"
        }
    ],
    "permissions":
    [
        "http://www.baidu.com/*",
        "https://www.baidu.com/*"
    ]
}